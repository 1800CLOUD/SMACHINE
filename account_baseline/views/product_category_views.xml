<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_category_property_form_account" model="ir.ui.view">
        <field name="name">product.category.view.form.inherit</field>
        <field name="model">product.category</field>
        <field name="inherit_id" ref="account.view_category_property_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='property_account_income_categ_id']" position="after">
                <field name="property_account_refund_categ_id" />
            </xpath>
        </field>
    </record>
</odoo>