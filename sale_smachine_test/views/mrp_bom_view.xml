<odoo>
    <record id="mrp_bom_form_view_inherit" model="ir.ui.view">
        <field name="name">mrp.bom.view.form.inherit</field>
        <field name="model">mrp.bom</field>
        <field name="inherit_id" ref="mrp.mrp_bom_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='type']" position="after">
                <field name="date_expiration" string="Date Expiration"/>
                <field name="quantity_available" string="Quantity Available"/>
            </xpath>
        </field>
    </record>
</odoo>